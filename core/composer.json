{
    "name": "gargi/domain",
    "description": "Domain of eCommerce app",
    "type": "library",
    "keywords": ["eCommerce", "eShop", "Domain/e-commerce"],
    "license": "Proprietary",
    "authors": [
      {
        "name": "Zoran Stevovic",
        "email": "zstevovich@gmail.com"
      }
    ],
    "autoload": {
      "psr-4": {
        "Core\\Application\\"  : "./Application",
        "Core\\Domain\\" : "./Domain",
        "Core\\Infrastructure\\" : "./Infrastructure"
      },
      "files": [

      ]
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
      "php": "^7.4|^8.0",
      "gedmo/doctrine-extensions": "^3.0",
      "laravel-doctrine/orm": "1.7.*",
      "laravel-doctrine/extensions": "1.3.*",
      "beberlei/doctrineextensions": "^1.3",
      "intervention/image": "^2.5",
      "roave/better-reflection": "^4.11",
      "ramsey/uuid-doctrine": "^1.6"
    },
    "require-dev": {
      "phpmetrics/phpmetrics": "^2.4"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Core\\Infrastructure\\CoreProvider"
        ],
        "aliases": {
        }
      },
      "branch-alias": {
        "dev-master": "1.0.0-dev"
      }
    },
    "config": {
      "sort-packages": true
    }
  }